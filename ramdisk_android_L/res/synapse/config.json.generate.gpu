cat << CTAG
{
	name:GPU,
		elements:[
			{ SPane:{
				title:"Current GPU Frequency:"
			}},
				{ SLiveLabel:{
					refresh:1000,
					style:italic,
					action:"live GPUFrequency"
				}},
			{ SPane:{
				title:"GPU Frequency Scaling",
			}},
				{ SSeekBar:{
					title:"GPU Min Frequency",
					description:"Set the minimum frequency the GPU scales down to.",
					default:`$BB cat \`sh $DEVICE DirGPUMinPwrLevel\``,
					action:"gpuset `sh $DEVICE DirGPUMinPwrLevel`",
					values:{
						`sh $DEVICE GPUPowerLevel`
					}
				}},
				{ SSeekBar:{
					title:"GPU Max Frequency",
					description:"Set the maximum frequency the GPU scales up to.",
					default:`$BB cat \`sh $DEVICE DirGPUMaxFrequency\``,
					action:"generic `sh $DEVICE DirGPUMaxFrequency`",
					values:{
						`sh $DEVICE GPUFrequencyList`
					}
				}},
			{ SPane:{
				title:"GPU Governor",
				description:"The GPU Governor makes scaling decisions based on specific algorithm for 2D/3D scenarios."
			}},
				{ SOptionList:{
					default:`sh $DEVICE DefaultGPUGovernor`,
					action:"gpuset `sh $DEVICE DirGPUGovernor`",
					values:[
						`sh $DEVICE GPUGovernorList`
					]
				}},
			`if [ -f "/sys/module/simple_gpu_algorithm/parameters/simple_laziness" ] || [ -f "/sys/module/simple_gpu_algorithm/parameters/simple_ramp_threshold" ]; then
				$BB echo '{ SPane:{
					title:"Simple Governor Tunables"
				}},'
				if [ -f "/sys/module/simple_gpu_algorithm/parameters/simple_gpu_activate" ]; then
					SIMPLE=\`$BB cat /sys/module/simple_gpu_algorithm/parameters/simple_gpu_activate/simple_gpu_activate\`
					$BB echo '{ SCheckBox:{
						label:"Enable",
						description:"An add-on for MSM Adreno TZ GPU Governor. Developed by faux123.",
						default:'$SIMPLE',
						action:"generic /sys/module/simple_gpu_algorithm/parameters/simple_gpu_activate"
					}},'
				fi
				if [ -f "/sys/module/simple_gpu_algorithm/parameters/simple_laziness" ]; then
					LAZ=\`$BB cat /sys/module/simple_gpu_algorithm/parameters/simple_laziness\`
					$BB echo '{ SSeekBar:{
						title:"Laziness",
						description:"Adjusts the number of times the governor skips ramp down requests. Higher = better performance (more battery drain).",
						default:'$LAZ',
						action:"generic /sys/module/simple_gpu_algorithm/parameters/simple_laziness",
						min:0,
						max:10,
						step:1
					}},'
				fi
				if [ -f "/sys/module/simple_gpu_algorithm/parameters/simple_ramp_threshold" ]; then
					RAMP=\`$BB cat /sys/module/simple_gpu_algorithm/parameters/simple_ramp_threshold\`
					$BB echo '{ SSeekBar:{
						title:"Ramp Threshold",
						description:"Adjusts the threshold to ramp up or down GPU frequencies. Lower = better performance (more battery drain).",
						default:'$RAMP',
						action:"generic /sys/module/simple_gpu_algorithm/parameters/simple_ramp_threshold",
						min:0,
						max:10000,
						step:1000
					}},'
				fi
			fi`
			`if [ -f "/sys/module/simple_gpu_algorithm/parameters/up_threshold" ] || [ -f "/sys/module/simple_gpu_algorithm/parameters/down_threshold" ] || [ -f "/sys/module/simple_gpu_algorithm/parameters/sample_time_ms" ]; then
				$BB echo '{ SPane:{
					title:"Interactive Governor Tunables"
				}},'
				if [ -f "/sys/module/simple_gpu_algorithm/parameters/down_threshold" ]; then
					DT=\`$BB cat /sys/module/simple_gpu_algorithm/parameters/down_threshold\`
					$BB echo '{ SSeekBar:{
						title:"Down Threshold",
						description:"When the GPU load is lower than this level, it ramps down the frequency one level.",
						default:'$DT',
						action:"generic /sys/module/simple_gpu_algorithm/parameters/down_threshold",
						min:0,
						max:100,
						step:1
					}},'
				fi
				if [ -f "/sys/module/simple_gpu_algorithm/parameters/up_threshold" ]; then
					UT=\`$BB cat /sys/module/simple_gpu_algorithm/parameters/up_threshold\`
					$BB echo '{ SSeekBar:{
						title:"Up Threshold",
						description:"When the GPU load is above this level, it ramps up the frequency one level.",
						default:'$UT',
						action:"generic /sys/module/simple_gpu_algorithm/parameters/up_threshold",
						min:0,
						max:100,
						step:1
					}},'
				fi
				if [ -f "/sys/module/simple_gpu_algorithm/parameters/sample_time_ms" ]; then
					ST=\`$BB cat /sys/module/simple_gpu_algorithm/parameters/sample_time_ms\`
					$BB echo '{ SSeekBar:{
						title:"Sample Time",
						description:"Sample time to ramp up or down GPU.",
						unit:" ms",
						default:'$ST',
						action:"generic /sys/module/simple_gpu_algorithm/parameters/sample_time_ms",
						min:0,
						max:1000,
						step:10
					}},'
				fi
			fi`
			`if [ -f "/sys/devices/system/cpu/cpu0/cpufreq/gpu_mv_table" ]; then
				$BB echo '{ SPane:{
					title:"GPU Voltages"
				}},'
					while read UVOLT UNIT; do
					VOLTAGE=$(($VOLTAGE+1));
						$BB echo '{ SSeekBar:{
							default:'$UVOLT',
							title:"Voltage '${VOLTAGE}'",
							action:"voltage gpuvolt '${VOLTAGE}'",
							unit:"' ${UNIT}'",'
							if [ "$UVOLT" -ge "900000" ]; then
								$BB echo 'min:900000, max:1200000, step:5000 }},'
							else
								$BB echo 'min:900, max:1200, step:5 }},'
							fi;
					done < /sys/devices/system/cpu/cpu0/cpufreq/gpu_mv_table
			fi`
		]
}
CTAG
